<div class="center jumbotron">
  <h1>Room detail</h1>
</div>
<div class="row">
  <div class="col-md-4">
    <% @count_order_rooms.each do |status, number| %>
      <p></label>Status: <label> <%= number %> <%= status %> </p>
    <% end %>
    <% @order_object = @room%>
    <label>
      <h3>Room info:</h3>
    </label></br>
  <%= render partial: "shared/#{@order_object.type_name}_info", locals: {object: @order_object}%>
</br>
</div>
<div class="col-md-8">
  <label>
    <h3>Orders history:</h3>
  </label></br>
<%@orders.each do |order|%>
  <%= render order %></br>
<%end%>
</div>
</div>
