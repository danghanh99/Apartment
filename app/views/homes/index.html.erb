<div class="center jumbotron">
  <h1>Apartment</h1>
</div>
<div class="row">
<%= form_tag(homes_path, :method => "get", class: 'navbar-form navbar-left') do %>  
  <%= search_field_tag :begin, params[:begin], placeholder: "from price", class: "form-control" %>
  <%= search_field_tag :end, params[:end], placeholder: "to price", class: "form-control" %> 
  <%= button_tag "", :class => 'btn btn-info glyphicon glyphicon-search',:name => nil%>  
<% end %>
</div>
<div class="row">
<% if @results %>
<%= render @results %></br>
<% end %>   
</div>
<%= link_to image_tag("chung-cu.png", alt: "Rails logo"),
            'http://rubyonrails.org/' %>
